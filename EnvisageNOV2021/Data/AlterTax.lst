3551   
3552  yc.fx(r,t)   = yc.l(r,t) ;
3553  yg.fx(r,t)   = yg.l(r,t) ;
3554  rsav.fx(r,t) = rsav.l(r,t) ;
3555  regY.fx(r,t) = (yc.l(r,t) + yg.l(r,t) + rsav.l(r,t)) ;
3556  regY0(r)     = 1 ;
3557  rsav0(r)     = 1 ;
3558  yg0(r)       = 1 ;
3559  yc0(r)       = 1 ;
3560  betaP0(r)    = 1 ;
3561  betaG0(r)    = 1 ;
3562  betaS0(r)    = 1 ;
