3503  * --------------------------------------------------------------------------------------------------
3504  *
3505  *  Income allocation module
3506  *
3507  * --------------------------------------------------------------------------------------------------
3508   
3509  *  Rescale eh0 !!!! DO WITH CAUTION
3510  if(1,
3511     loop((h,t0),
3512        eh0(r,i) = eh0(r,i)/sum(j, eh0(r,j)*xcshr.l(r,j,h,t0)) ;
3513     ) ;
3514  ) ;
3515   
3516  eh.fx(r,i,t) = eh0(r,i) ;
3517  bh.fx(r,i,t) = bh0(r,i) ;
3518  phiP.fx(r,t) = sum((i,h), xcshr.l(r,i,h,t)*eh.l(r,i,t))$(CD eq CDE)
3519               + sum((i,h), xcshr.l(r,i,h,t))$(CD eq CD) ;
3520   
3521  display eh.l, bh.l, eh0, bh0, phiP.l, xcshr.l ;
3522   
3523  kron(is,is) = 1 ; display kron ;
3524   
3525  loop(h,
3526     ued.l(r,i,j,t)   = xcshr.l(r,i,h,t)*(-bh.l(r,i,t)
3527                      - (eh.l(r,i,t)*bh.l(r,i,t)
3528                      - sum(jp, xcshr.l(r,jp,h,t)*eh.l(r,jp,t)*bh.l(r,jp,t)))
3529                      /  sum(jp, xcshr.l(r,jp,h,t)*eh.l(r,jp,t))) + kron(i,j)*(bh.l(r,i,t) - 1) ;
3530   
3531     incelas.l(r,i,t) = (eh.l(r,i,t)*bh.l(r,i,t)
3532                      - sum(jp, xcshr.l(r,jp,h,t)*eh.l(r,jp,t)*bh.l(r,jp,t)))
3533                      /   sum(jp, xcshr.l(r,jp,h,t)*eh.l(r,jp,t))
3534                      -  (bh.l(r,i,t) - 1) + sum(jp, xcshr.l(r,jp,h,t)*bh.l(r,jp,t)) ;
3535   
3536     ced.l(r,i,j,t)   = ued.l(r,i,j,t) + xcshr.l(r,j,h,t) * incelas.l(r,i,t) ;
3537   
3538     ape.l(r,i,j,t)$xcshr.l(r,j,h,t)
3539                      = 1 - bh.l(r,j,t) - bh.l(r,i,t) + sum(jp, xcshr.l(r,jp,h,t)*bh.l(r,jp,t))
3540                      -  kron(i,j)*(1-bh.l(r,i,t))/xcshr.l(r,j,h,t) ;
3541  ) ;
