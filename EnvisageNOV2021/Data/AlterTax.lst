1570  *  -------------------------------------------------------------------------------------------------
1571   
1572  *  (P-1) Demand for aggregate value added -- (E_qva (997), VADEMAND (1262))
1573   
1574  vaeq(r,a,t)$(rs(r) and ts(t) and vaFlag(r,a))..
1575     va(r,a,t) =e= xp(r,a,t)*(px(r,a,t)/pva(r,a,t))**sigmap(r,a)
1576                *  (axp(r,a,t)*lambdava(r,a,t))**(sigmap(r,a)-1) ;
1577   
1578  *  (P-2) Top level nest -- (E_qint (983), no equivalent)
1579   
1580  ndeq(r,a,t)$(rs(r) and ts(t) and ndFlag(r,a))..
1581     nd(r,a,t) =e= xp(r,a,t)*(px(r,a,t)/pnd(r,a,t))**sigmap(r,a)
1582                *  (axp(r,a,t)*lambdand(r,a,t))**(sigmap(r,a)-1) ;
1583   
1584  *  Unit cost definition (also zero profit condition) -- (E_qo (1027), ZEROPROFITS (1464))
1585   
1586  pxeq(r,a,t)$(rs(r) and ts(t) and xpFlag(r,a))..
1587     px(r,a,t)**(1-sigmap(r,a)) =e= (axp(r,a,t)**(sigmap(r,a)-1))
1588        *  (ava(r,a,t)*(pva(r,a,t)/lambdava(r,a,t))**(1-sigmap(r,a))
1589        +   and(r,a,t)*(pnd(r,a,t)/lambdand(r,a,t))**(1-sigmap(r,a))) ;
1590   
1591  *  (P-4) Demand for value added -- (E_qfe (1103), ENDWDEMAND (1423))
1592   
1593  xfeq(r,fp,a,t)$(rs(r) and ts(t) and xfFlag(r,fp,a))..
1594     xf(r,fp,a,t) =e= va(r,a,t)*(pva(r,a,t)/(((pf0(r,fp,a)*pf(r,fp,a,t)*(1 + fctts(r,fp,a,t) + fcttx(r,fp,a,t))) /pfa0(r,fp,a))$ifSUB + (pfa(r,fp,
      a,t))$(not ifSUB)))**sigmav(r,a)
1595                   *  (lambdaf(r,fp,a,t))**(sigmav(r,a)-1) ;
1596   
1597  *  (P-5) Price of value added bundle -- (E_pva (1110), VAPRICE (1403))
1598   
1599  pvaeq(r,a,t)$(rs(r) and ts(t) and vaFlag(r,a))..
1600     pva(r,a,t)**(1-sigmav(r,a)) =e=
1601        sum(fp, af(r,fp,a,t)*((((pf0(r,fp,a)*pf(r,fp,a,t)*(1 + fctts(r,fp,a,t) + fcttx(r,fp,a,t))) /pfa0(r,fp,a))$ifSUB + (pfa(r,fp,a,t))$(not ifS
      UB))/lambdaf(r,fp,a,t))**(1-sigmav(r,a))) ;
1602   
1603  *  Intermediate demand nest
1604   
1605  *  (P-6) Demand for intermediates -- (E_qfa (1052), ~INTDEMAND (1286))
1606   
1607  xapeq(r,i,a,t)$(rs(r) and ts(t) and xaFlag(r,i,a))..
1608     xa(r,i,a,t) =e= nd(r,a,t)*(pnd(r,a,t)/pa(r,i,a,t))**sigmand(r,a)
1609                  *  lambdaio(r,i,a,t)**(sigmand(r,a)-1) ;
1610   
1611  *  (P-7) Price if ND bundle -- (E_pint (1071), no equivalent)
1612   
