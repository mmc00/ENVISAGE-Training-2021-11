3543  *  Initialize parameters
3544   
3545  betaP.l(r,t) = yc.l(r,t)/regY.l(r,t) ;
3546  betaG.l(r,t) = yg.l(r,t)/regY.l(r,t) ;
3547  betaS.l(r,t) = rsav.l(r,t)/regY.l(r,t) ;
3548  phi.l(r,t)   = 1/(phiP.l(r,t)*betaP.l(r,t) + betaG.l(r,t) + betaS.l(r,t)) ;
3549   
