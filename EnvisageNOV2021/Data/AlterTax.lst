2502     pgdpmp(r,t) =e= gdpmp(r,t)/rgdpmp(r,t) ;
2503   
2504  *  Equivalent variation (E(p0,u))
2505   
2506  eveq(r,h,t)$(rs(r) and ts(t))..
2507     0 =e= (sum(i$xaFlag(r,i,h), alphaa(r,i,h,t)*(uh(r,h,t)**(bh(r,i,t)*eh(r,i,t)))
2508        *   (sum(t0, pa0(r,i,h)*pa(r,i,h,t0))*Pop(r,t)/(ev0(r,h)*ev(r,h,t)))**bh(r,i,t)) - 1)
2509        $(CD eq CDE)
2510   
2511        +  (uh(r,h,t)*sum(t0, yc0(r)*yc(r,t0)) - ev0(r,h)*ev(r,h,t))
2512        $(CD eq CD)
2513        ;
2514   
2515  *  Compensating variation (E(p,u0))
2516   
2517  cveq(r,h,t)$(rs(r) and ts(t) and (CD eq CDE))..
2518     sum(i$xaFlag(r,i,h), alphaa(r,i,h,t)*(sum(t0, uh(r,h,t0))**(bh(r,i,t)*eh(r,i,t)))
2519            * (pa0(r,i,h)*pa(r,i,h,t)*Pop(r,t)/(cv0(r,h)*cv(r,h,t)))**bh(r,i,t)) =e= 1 ;
2520   
2521  * --------------------------------------------------------------------------------------------------
2522  *
2523  *     Dynamic equations
2524  *
2525  * --------------------------------------------------------------------------------------------------
2526   
2527  *  Top level uniform productivity shifter -- AOWORLD (1240)
2528   
2529  axpeq(r,a,t)$(rs(r) and ts(t) and xpFlag(r,a))..
2530     axp(r,a,t) =e= axp(r,a,t-1)*(1 + axpsec(a,t) + axpreg(r,t) + axpall(r,a,t)) ;
2531   
2532  *  ND bundle shifter -- no equivalent (TBV)
2533   
2534  lambdandeq(r,a,t)$(rs(r) and ts(t) and ndFlag(r,a))..
2535     lambdand(r,a,t) =e= lambdand(r,a,t-1)*(1 + andsec(a,t) + andreg(r,t) + andall(r,a,t)) ;
2536   
2537  *  VA bundle shifter -- AVAWORLD (1251)
2538   
2539  lambdavaeq(r,a,t)$(rs(r) and ts(t) and vaFlag(r,a))..
2540     lambdava(r,a,t) =e= lambdava(r,a,t-1)*(1 + avasec(a,t) + avareg(r,t) + avaall(r,a,t)) ;
2541   
2542  *  Factor demand technical change -- AFEWORLD (1382)
2543   
2544  lambdafeq(r,fp,a,t)$(rs(r) and ts(t) and xfFlag(r,fp,a))..
2545     lambdaf(r,fp,a,t) =e= lambdaf(r,fp,a,t-1)
2546        * (1 + afecom(fp,t) + afesec(a,t) + afefac(r,fp,t)
2547        + afereg(r,t)$afeFlag(r,fp) + afeall(r,fp,a,t)) ;
2548   
2549  *  Intermediate augmenting technical change -- AFWORLD (1281) with adjustments
2550   
2551  lambdaioeq(r,i,a,t)$(rs(r) and ts(t) and xaFlag(r,i,a))..
2552     lambdaio(r,i,a,t) =e= lambdaio(r,i,a,t-1)
2553        *(1 + aiocom(i,t) + aiosec(a,t) + aioreg(r,t) + aioall(r,i,a,t)) ;
2554   
2555  *  Real GDP growth
2556   
2557  glcaleq(r,t)$(rs(r) and ts(t) and years(t) gt FirstYear)..
2558     rgdpmp(r,t) =e= (1 + ggdppc(r,t))*rgdpmp(r,t-1)*(pop(r,t)/pop(r,t-1)) ;
2559   
2560  *  Labor productivity
2561   
2562  afealleq(r,l,a,t)$(ifCal eq 1 and rs(r) and ts(t) and years(t) gt FirstYear and xfFlag(r,l,a))..
2563     afeall(r,l,a,t) =e= piadd(r,l,a,t) + pimlt(r,l,a,t)*gl(r,t) ;
2564   
2565  *  Other equations
2566   
2567  uedeq(r,i,j,h,t)$(rs(r) and ts(t) and xaFlag(r,i,h) ne 0 and xaFlag(r,j,h) and (CD eq CDE))..
2568     ued(r,i,j,t)
2569        =e= xcshr0(r,i,h)*xcshr(r,i,h,t)*(-bh(r,i,t)
2570         - (eh(r,i,t)*bh(r,i,t) - sum(jp, xcshr0(r,jp,h)*xcshr(r,jp,h,t)*eh(r,jp,t)*bh(r,jp,t)))
2571         /  sum(jp, xcshr0(r,jp,h)*xcshr(r,jp,h,t)*eh(r,jp,t))) + kron(i,j)*(bh(r,i,t) - 1) ;
2572   
2573  incelaseq(r,i,h,t)$(rs(r) and ts(t) and xaFlag(r,i,h) and (CD eq CDE))..
2574     incelas(r,i,t)
2575        =e= (eh(r,i,t)*bh(r,i,t) - sum(jp, xcshr0(r,jp,h)*xcshr(r,jp,h,t)*eh(r,jp,t)*bh(r,jp,t)))
2576         /   sum(jp, xcshr0(r,jp,h)*xcshr(r,jp,h,t)*eh(r,jp,t))
2577         -  (bh(r,i,t) - 1) + sum(jp, xcshr0(r,jp,h)*xcshr(r,jp,h,t)*bh(r,jp,t)) ;
2578   
2579  cedeq(r,i,j,h,t)$(rs(r) and ts(t) and xaFlag(r,i,h) ne 0 and xaFlag(r,j,h) and (CD eq CDE))..
2580     ced(r,i,j,t) =e= ued(r,i,j,t) + xcshr0(r,j,h)*xcshr(r,j,h,t) * incelas(r,i,t) ;
2581   
2582  apeeq(r,i,j,h,t)$(rs(r) and ts(t) and xaFlag(r,i,h) ne 0 and xaFlag(r,j,h) and (CD eq CDE))..
2583     ape(r,i,j,t) =e= 1 - bh(r,j,t) - bh(r,i,t) + sum(jp, xcshr0(r,jp,h)*xcshr(r,jp,h,t)*bh(r,jp,t))
2584                   -  kron(i,j)*(1-bh(r,i,t))/(xcshr0(r,j,h)*xcshr(r,j,h,t)) ;
