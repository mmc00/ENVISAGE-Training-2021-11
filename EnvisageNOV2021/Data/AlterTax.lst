3494  * --------------------------------------------------------------------------------------------------
3495  *
3496  *  MRIO module
3497  *
3498  * --------------------------------------------------------------------------------------------------
3499   
3500  pma0(r,i,aa)   = 1 ;
3501  xwa0(s,i,d,aa) = 0 ;
