2595     pdma(s,i,r,aa,t) =e= (1 + imptxa(s,i,r,aa,t) + mtax(r,i,t))*(((((((1 + exptx(s,i,r,t) + etax(s,i,t))*pe0(s,i,r)*pe(s,i,r,t)) /pefob0(s,i,r))$
      ifSUB + (pefob(s,i,r,t))$(not ifSUB))*pefob0(s,i,r) + pwmg0(s,i,r)*(((sum(m, amgm(m,s,i,r)*ptmg0(m)*ptmg(m,t)/lambdamg(m,s,i,r,t))) /pwmg0(s,i,r
      ))$ifSUB + (pwmg(s,i,r,t))$(not ifSUB))*tmarg(s,i,r,t)) /pmcif0(s,i,r))$ifSUB + (pmcif(s,i,r,t))$(not ifSUB))
2596                       *     pmcif0(s,i,r)/pdma0(s,i,r,aa) ;
