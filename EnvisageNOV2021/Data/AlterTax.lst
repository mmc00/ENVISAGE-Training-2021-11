3331  * --------------------------------------------------------------------------------------------------
3332  *
3333  *  Income distribution
3334  *
3335  * --------------------------------------------------------------------------------------------------
3336   
3337  YTAX.l(r,"pt",t) = sum((a,i), prdtx.l(r,a,i,t)*p.l(r,a,i,t)*x.l(r,a,i,t)) ;
3338  YTAX.l(r,"fc",t) = sum((i,a), dintx.l(r,i,a,t)*pd.l(r,i,t)*xd.l(r,i,a,t)
3339                   +            mintx.l(r,i,a,t)*pmt.l(r,i,t)*xm.l(r,i,a,t)) ;
3340  YTAX.l(r,"pc",t) = sum((i,h), dintx.l(r,i,h,t)*pd.l(r,i,t)*xd.l(r,i,h,t)
3341                   +            mintx.l(r,i,h,t)*pmt.l(r,i,t)*xm.l(r,i,h,t)) ;
3342  YTAX.l(r,"gc",t) = sum((i,gov), dintx.l(r,i,gov,t)*pd.l(r,i,t)*xd.l(r,i,gov,t)
3343                   +              mintx.l(r,i,gov,t)*pmt.l(r,i,t)*xm.l(r,i,gov,t)) ;
3344  YTAX.l(r,"ic",t) = sum((i,inv), dintx.l(r,i,inv,t)*pd.l(r,i,t)*xd.l(r,i,inv,t)
3345                   +              mintx.l(r,i,inv,t)*pmt.l(r,i,t)*xm.l(r,i,inv,t)) ;
3346  YTAX.l(r,"et",t) = sum((i,d), (exptx.l(r,i,d,t) + etax.l(r,i,t))*pe.l(r,i,d,t)*xw.l(r,i,d,t)) ;
3347  YTAX.l(r,"mt",t) = sum((i,s), (imptx.l(s,i,r,t)
3348                   +   mtax.l(r,i,t))*pmcif.l(s,i,r,t)*xw.l(s,i,r,t)) ;
3349  YTAX.l(r,"ft",t) = sum((fp,a), fcttx.l(r,fp,a,t)*pf.l(r,fp,a,t)*xf.l(r,fp,a,t)) ;
3350  YTAX.l(r,"fs",t) = sum((fp,a), fctts.l(r,fp,a,t)*pf.l(r,fp,a,t)*xf.l(r,fp,a,t)) ;
3351   
3352  YTAX.l(r,"dt",t) = sum((a,fp), kappaf.l(r,fp,a,t)*pf.l(r,fp,a,t)*xf.l(r,fp,a,t)) ;
3353   
3354  yTaxTot.l(r,t)   = sum(gy, ytax.l(r,gy,t)) ;
3355  yTaxInd.l(r,t)   = yTaxTot.l(r,t) - ytax.l(r,"dt",t) ;
3356  kstock.l(r,t)    = inScale*VKB(r) ;
3357  fdepr(r,t)       = inScale*VDEP(r)/(pi.l(r,t)*kstock.l(r,t)) ;
3358  depr(r,t)        = fdepr(r,t) ;
3359  loop(cap,
3360     krat(r,t)     = xft.l(r,cap,t)/kstock.l(r,t) ;
3361  ) ;
3362   
3363  loop((h,inv,gov),
3364     rsav.l(r,t) = inScale*SAVE(r) ;
3365     savf.l(r,t) = sum((i,s), pmCIF.l(s,i,r,t)*xw.l(s,i,r,t))
3366                 - sum((i,d), peFOB.l(r,i,d,t)*xw.l(r,i,d,t))
3367                 - sum(i0, inScale*vst(i0,r)) ;
3368  ) ;
3369   
3370  work = sum((r,t0)$(not rres(r)), savf.l(r,t0)) ;
3371  savf.l(rres,t) = -work ;
3372   
3373  xigbl.l(t)     = sum(r, xi.l(r,t) - depr(r,t)*kstock.l(r,t)) ;
3374  chiInv.l(r,t)  = (xi.l(r,t) - depr(r,t)*kstock.l(r,t))/xigbl.l(t) ;
3375  pigbl.l(t)     = sum(r, pi.l(r,t)*(xi.l(r,t) - depr(r,t)*kstock.l(r,t)))/xigbl.l(t) ;
3376  invwgt(r,t)    = pi.l(r,t)*(xi.l(r,t) - depr(r,t)*kstock.l(r,t))
3377                 / sum(rp, pi.l(rp,t)*(xi.l(rp,t) - depr(rp,t)*kstock.l(rp,t))) ;
3378  savwgt(r,t)    = rsav.l(r,t) / sum(rp, rsav.l(rp,t)) ;
3379  chiSave.l(t)   = 1 ;
3380  psave.l(r,t)   = 1 ;
3381  factY.l(r,t)   = sum((fp,a), pf.l(r,fp,a,t)*xf.l(r,fp,a,t)) - fdepr(r,t)*pi.l(r,t)*kstock.l(r,t) ;
3382  regY.l(r,t)    = factY.l(r,t) + yTaxInd.l(r,t) ;
3383  chif.l(r,t)    = savf.l(r,t) / regY.l(r,t) ;
3384   
3385  savfBar(r,t)   = savf.l(r,t)/pigbl.l(t) ;
