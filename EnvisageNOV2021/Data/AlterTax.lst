3260        imptxa.l(s,i,d,h,t)$imptxa.l(s,i,d,gov,t)
3261           = (inscale*sum(i0$mapi0(i0,i), viums(i0, "CONS", s ,d)))/imptxa.l(s,i,d,gov,t) - 1.0 ;
3262        imptxa.l(s,i,d,gov,t)$imptxa.l(s,i,d,gov,t)
3263           = (inscale*sum(i0$mapi0(i0,i), viums(i0, "CONS", s ,d)))/imptxa.l(s,i,d,gov,t) - 1.0 ;
3264     ) ;
3265     loop(inv,
