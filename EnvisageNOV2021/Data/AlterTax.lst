3413  * --------------------------------------------------------------------------------------------------
3414  *
3415  *  Initialize model parameters
3416  *
3417  * --------------------------------------------------------------------------------------------------
3418   
3419  loop(t0,
3420   
3421  *  sigmap is top level subsitution elasticity, by default equal to esubt
3422  *  use production weights
3423   
3424     tvom(a0,r) = sum(i0, vdfp(i0,a0,r) + vmfp(i0,a0,r)) + sum(fp, evfp(fp,a0,r)) ;
3425     sigmap(r,a)$(sigmap(r,a) eq na and xp.l(r,a,t0))
3426        = sum(a0$mapa0(a0,a), tvom(a0,r)*esubt(a0,r))/sum(a0$mapa0(a0,a), tvom(a0,r)) ;
3427   
3428  *  sigmand by default is equal to esubc
3429   
3430     tvom(a0,r) = sum(i0, vdfp(i0,a0,r) + vmfp(i0,a0,r)) ;
3431     sigmand(r,a)$(sigmand(r,a) eq na and nd.l(r,a,t0))
3432        = sum(a0$mapa0(a0,a), tvom(a0,r)*esubc(a0,r))/sum(a0$mapa0(a0,a), tvom(a0,r)) ;
3433   
3434  *  sigmav is subsitution across factors, by default equal to esubva
3435  *  use value added weights
3436     tvom(a0,r) = sum(fp, evfp(fp,a0,r)) ;
3437     sigmav(r,a)$(sigmav(r,a) eq na and va.l(r,a,t0))
3438        = sum(a0$mapa0(a0,a), tvom(a0,r)*esubva(a0,r))/sum(a0$mapa0(a0,a), tvom(a0,r)) ;
3439   
3440  *  sigmam is region and agent specific, by default set to esubd
3441   
3442     sigmam(r,i,a)$(sigmam(r,i,a) eq na and xa.l(r,i,a,t0))
3443        = sum((i0,a0)$(mapi0(i0,i) and mapa0(a0,a)), (vdfp(i0,a0,r)+vmfp(i0,a0,r))*esubd(i0,r))
3444        / sum((i0,a0)$(mapi0(i0,i) and mapa0(a0,a)), (vdfp(i0,a0,r)+vmfp(i0,a0,r))) ;
3445     sigmam(r,i,h)$(sigmam(r,i,h) eq na and xa.l(r,i,h,t0))
3446        = sum(i0$mapi0(i0,i), (vdpp(i0,r)+vmpp(i0,r))*esubd(i0,r))
3447        / sum(i0$mapi0(i0,i), (vdpp(i0,r)+vmpp(i0,r))) ;
3448     sigmam(r,i,gov)$(sigmam(r,i,gov) eq na and xa.l(r,i,gov,t0))
3449        = sum(i0$mapi0(i0,i), (vdgp(i0,r)+vmgp(i0,r))*esubd(i0,r))
3450        / sum(i0$mapi0(i0,i), (vdgp(i0,r)+vmgp(i0,r))) ;
3451     sigmam(r,i,inv)$(sigmam(r,i,inv) eq na and xa.l(r,i,inv,t0))
3452        = sum(i0$mapi0(i0,i), (vdip(i0,r)+vmip(i0,r))*esubd(i0,r))
3453        / sum(i0$mapi0(i0,i), (vdip(i0,r)+vmip(i0,r))) ;
3454   
3455  *  sigmam for trade margins does not exist in the standard GTAP model and is mostly
3456  *  irrelevant since there are no imports for this activity. Set it to the investment elasticity
3457   
3458     loop(h,
3459        sigmam(r,i,tmg)$(sigmam(r,i,tmg)) = sigmam(r,i,h) ;
3460        sigmamt(r,i) = sigmam(r,i,h) ;
3461     ) ;
3462   
3463  *  sigmaw is second level Armington subsitution elasticity, by default equal to esubm
3464  *  use import weights
3465   
3466     sigmaw(r,i)$(sigmaw(r,i) eq na and xmt.l(r,i,t0))
3467        = sum((i0,s)$mapi0(i0,i), vcif(i0, s, r)*esubm(i0,r))
3468        / sum((i0,s)$mapi0(i0,i), vcif(i0, s, r)) ;
3469   
3470     sigmawa(r,i,aa)$(sigmawa(r,i,aa) eq na)
3471        = sigmaw(r,i) ;
3472   
3473  *  eh0 and bh0 are the CDE expansion and substitution parameters, by default equal
3474  *  to incpar and subpar respectively. Use consumption weights
3475   
3476     loop(h,
3477        eh0(r,i)$(eh0(r,i) eq na and xa.l(r,i,h,t0))
3478           = sum(i0$mapi0(i0,i), incpar(i0,r)*(vdpp(i0,r) + vmpp(i0,r)))
3479           / sum(i0$mapi0(i0,i), (vdpp(i0,r) + vmpp(i0,r))) ;
3480        bh0(r,i)$(bh0(r,i) eq na and xa.l(r,i,h,t0))
3481           = sum(i0$mapi0(i0,i), subpar(i0,r)*(vdpp(i0,r) + vmpp(i0,r)))
3482           / sum(i0$mapi0(i0,i), (vdpp(i0,r) + vmpp(i0,r))) ;
3483   
3484        eh0(r,i)$(eh0(r,i) eq na) = 0 ;
3485        bh0(r,i)$(bh0(r,i) eq na) = 0 ;
3486     ) ;
3487   
3488  *  The GEMPACK version assumes CET elasticities are negative
3489   
3490     omegaf(r,fp)$(omegaf(r,fp) eq na) = -etrae(fp,r)$(etrae(fp,r) ne inf)
3491                                       + INF$(etrae(fp,r) eq inf) ;
3492  ) ;
